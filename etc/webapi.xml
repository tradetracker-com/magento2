<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/tradetracker/moduleSettings/:storeId">
        <service class="TradeTracker\Connect\Api\Webapi\ManagementInterface" method="getModuleSettings"/>
        <resources>
            <resource ref="TradeTracker_Connect::webapi"/>
        </resources>
    </route>
</routes>
